      </tuv>
      <tuv lang="pl" changeid="damia" changedate="20231101T110205Z">
        <seg>Spójrz na:</seg>
      </tuv>
    </tu>
    <tu>
      <tuv lang="en-US">
        <seg>Look no further!</seg>
      </tuv>
