      <tuv lang="pl" changeid="damia" changedate="20231224T190256Z" creationid="damia" creationdate="20231224T190256Z">
        <seg>Edgar E. Dangerous</seg>
      </tuv>
    </tu>
    <tu>
      <tuv lang="en-US">
        <seg>Edward M. Beaux</seg>
      </tuv>
      <tuv lang="pl" changeid="damia" changedate="20231224T190256Z" creationid="damia" creationdate="20231224T190256Z">
        <seg>Edward M. Beaux</seg>
      </tuv>
    </tu>
    <tu>
      <tuv lang="en-US">
        <seg>Eeee Eeee Eeee!</seg>
      </tuv>
      <tuv lang="pl" changeid="damia" changedate="20231013T102343Z" creationid="damia" creationdate="20231013T102343Z">
