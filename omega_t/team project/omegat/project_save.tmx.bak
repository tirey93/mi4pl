    <tu>
      <tuv lang="en-US">
        <seg>Ale teraz będzie wybuch odstraszający.</seg>
      </tuv>
      <tuv lang="pl" changeid="damia" changedate="20231029T113211Z" creationid="damia" creationdate="20231029T113211Z">
        <seg>Ale teraz będzie wybuch odstraszający.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv lang="en-US">
        <seg>Alibi, shmalibi.</seg>
      </tuv>
      <tuv lang="pl" changeid="Pytkowscy" changedate="20130728T112223Z">
