msgctxt "guymon005"
msgid "En garde, monkey!"
msgstr "En garde, małpo!"

msgctxt "guymon006"
msgid "Let's rumble!"
msgstr "Zaraz pożałujesz!"

msgctxt "guymon007"
msgid "Let's see what you got."
msgstr "Pokaż na co cię stać."

msgctxt "guymon008"
msgid "Uh, never mind."
msgstr "Uh, nieważne."

msgctxt "guymon009"
msgid "Sorry I stopped ya."
msgstr "Zatrzymałem cię."

msgctxt "guymon010"
msgid "Go about your business."
msgstr "Pogadajmy o tobie."

msgctxt "jojmon008"
msgid "You are indeed ready to fight me."
msgstr "Rzeczywiście możesz się ze mną zmierzyć."

msgctxt "jojmon009"
msgid "Let's see if you can beat the master."
msgstr "Zobaczmy czy pokonasz mistrza."

msgctxt "jojmon010"
msgid "Bring it on, flesh-boy."
msgstr "Dawaj, ciamajdo."

msgctxt "jojmon011"
msgid "You are good, but are you good enough?"
msgstr "Dobry jesteś, ale czy wystarczająco?"

msgctxt "mk1mon005"
msgid "Whoo oooh ooh!"
msgstr ""

msgctxt "mk2mon005"
