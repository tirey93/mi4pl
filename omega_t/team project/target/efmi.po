msgctxt "guymon005"
msgid "En garde, monkey!"
msgstr "En garde, małpo!"

msgctxt "guymon006"
msgid "Let's rumble!"
msgstr "Czas na walkę!"

msgctxt "guymon007"
msgid "Let's see what you got."
msgstr "Pokaż na co cię stać."

msgctxt "guymon008"
msgid "Uh, never mind."
msgstr "Uh, nieważne."

msgctxt "guymon009"
msgid "Sorry I stopped ya."
msgstr "Zatrzymałem cię."

msgctxt "guymon010"
msgid "Go about your business."
msgstr "Zajmij się swoimi sprawami."

msgctxt "jojmon008"
msgid "You are indeed ready to fight me."
msgstr "Rzeczywiście jesteś gotowy na walkę ze mną."

msgctxt "jojmon009"
msgid "Let's see if you can beat the master."
msgstr "Zobaczymy, czy uda ci się pokonać mistrza."

msgctxt "jojmon010"
msgid "Bring it on, flesh-boy."
msgstr "No dalej, ciamajdo."

msgctxt "jojmon011"
msgid "You are good, but are you good enough?"
msgstr "Jesteś dobry, ale czy wystarczająco dobry?"

msgctxt "mk1mon005"
msgid "Whoo oooh ooh!"
msgstr ""

msgctxt "mk2mon005"
msgid "Whoo oooh ooh!"
msgstr ""

msgctxt "mk3mon005"
msgid "Whoo oooh ooh!  Whooh oooh ooh!"
msgstr ""

